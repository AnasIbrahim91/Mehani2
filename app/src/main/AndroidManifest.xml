<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mehani.wyanbu.com.mehani">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainCareers"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Careers_Fields"
            android:label="@string/title_activity_careers__fields"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".User_Order"
            android:label="@string/title_activity_user__order"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Register"></activity>
    </application>

</manifest>